# Test: File menu validation
# Validates File menu opens and contains expected items

# Open File menu
menu_open File
wait 2

# Validate menu is open
validate menu_open=File

# Validate expected menu items exist
validate menu_contains=New
validate menu_contains=Open
validate menu_contains=Save
validate menu_contains=Save As
validate menu_contains=Page Setup
validate menu_contains=Print
validate menu_contains=Exit

# Close menu
key ESCAPE
wait 2
validate menu_open=false

